<main>
   {{!-- header nav --}}
   <div class="card mb-3">
   {{#with navigation}}
      <div class="nav">
      {{#each links}}
         {{#if href}}
         <div class="nav-item">
            <a href="href" class="nav-link">{{text}}</a>
         </div>
         {{else}}
         <div class="nav-item">
            <a href="href" class="nav-link disabled">Disabled</a>
         </div>
         {{/if}}
      {{/each}}             
      </div>
   {{/with}}
   </div>
   <hr>
   {{!--part 1--}}
   <div class="breadcrumb my-5">
      {{#each breadcrumb}}
      <a href="{{url}}" class="breadcrumb-item"> {{name}} </a>
      {{/each}}
   </div>
   <h1 class="h1"> {{header}} </h1>
   <div class="row mx-0 g-2">
      {{#each sortBlock}}
      <div class="col-3">
         <form action="" class="">
            <select name="" id="" class="form-select">
               <option value="" selected>{{placeholder}}</option>
               {{#each options}}   
               <option value="{{value}}">{{text}}</option>
               {{/each}}
            </select>
         </form>
      </div>
      {{/each}}
   </div>
   {{!--end of part 1--}}
   <div class="row g-2">
      <div class="card col-4 g-2">
      {{#with filterBlock}}
         <div class="card-body">
            {{#with sellers}}
            <div>
               <p class="h3"> {{title}} </p>
               <form action="" class="form-check">
               {{#each list}}
                  <div class="my-1">
                  <input type="checkbox" class="form-check-input" name="">
                  <label for="" class="form-check-label"> {{value}} </label>
                  </div>
               {{/each}}
               </form>               
            {{/with}}
            </div>
            <div>
               <hr class="my-3">
            {{#with brands}}
               <p class="h3"> {{title}} </p>
               <form action="" class="">
                  {{#with input}}
                  <input type="text" name="" id="" class="form-control" placeholder="{{placeholder}}">
                  {{/with}}
               </form>
               <form action="" class="form-check">
                  {{#each options}}
                  <div class="my-1">
                  <input type="checkbox" class="form-check-input" name="">
                  <label for="" class="form-check-label"> {{value}} </label>
                  </div>
                  {{/each}}
               </form>
            {{/with}}
            </div>            
            <div>
               <hr class="my-3">
               {{#with price}}
               <p class="h3">{{title}}</p>
               <form action="" class="m-0">
                  {{#with range}}
                  <input type="text" name="" id="" class="form-control my-3" placeholder="{{from}}">
                  <input type="text" name="" id="" class="form-control my-3" placeholder="{{to}}">
                  {{/with}}
               </form>
                  {{#with button}}
                  <a href="{{link}}" class="btn btn-outline-secondary">{{text}}</a>
                  {{/with}}
               {{/with}}
            </div>
            <div>
            {{#each filterList}}
               <hr class="my-3">
               <p class="h3"> {{title}} </p>
               <form action="" class="form-check">
                  {{#each options}}
                  <div class="my-2">
                     <input type="checkbox" name="" id="" class="form-check-input">
                     <label for="" class="form-check-label">{{value}}</label>
                  </div>
                  {{/each}}
               </form>
            {{/each}}
            </div>
         </div>
      {{/with}} {{!--close filterBlock--}}
      </div> {{!--close left side--}}
      <div class="col-8">
         <div class="row mx-0 g-2">
      {{#each goodsList}}
         {{> catalogItem}}
      {{/each}}
         </div>
      </div>
   </div>
   <hr class="my-3">
   <div class="row mx-0">
      <div class="card text-bg-success col-12">
      {{#with service}}
         <div class="card-body text-center">
            <p class="h2"> {{title}} </p>
            <p class="paragraph"> {{description}} </p>
            {{#each buttons}}
               <a href="{{link}}" class="btn btn-primary"> {{text}} </a>
            {{/each}}
         </div>
      {{/with}}
      </div>
   </div>   


   {{!-- footer nav --}}
   <hr class="my-3">
   <div class="row my-3">
      {{#each footer}}   
         <div class="col-2">
            <div class="nav flex-column">
               <a href="{{0.link}}" class="nav-item nav-link">{{0.text}}</a>
               <a href="{{1.link}}" class="nav-item nav-link">{{1.text}}</a>
               <a href="{{2.link}}" class="nav-item nav-link">{{2.text}}</a>
               <a href="{{3.link}}" class="nav-item nav-link disabled">{{3.text}}</a>
            </div>
         </div>
      {{/each}}
   </div>
   <hr>
</main>